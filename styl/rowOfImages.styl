    width 100%


.imagesFlex
  display flex
  flex-direction row
  justify-content center
  flex-wrap nowrap
  //align-content stretch
  padding paddingValueWidth
  &Item
    //padding paddingValueHeight
    width 33.33%
    overflow hidden


.imagesWithRef
  width 33.33%
  a
    .imagesFlexItem
      width 100%
      height 100%